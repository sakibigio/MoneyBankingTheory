  journal             = {The American Economic Review},
  title               = {Adverse Selection in Durable Goods Markets},
  year                = {1999},
  number              = {5},
  pages               = {1097-1115},
  volume              = {89},
  abstract            = {We present a dynamic model of adverse selection to examine the interactions
